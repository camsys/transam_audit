- if can? :create, Asset
  - FileContentType.active.each do |fc|
    .btn-group
      = button_to create_template_uploads_path, :title => 'Export assets to template', :role => 'button', :class => "btn btn-default selected-action", :params => {:file_content_type_id => fc.id, :targets => ""} do
        %i.fa.fa-download.fa-1-5x
        = fc.name
