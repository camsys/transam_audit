- if can? :create, Asset
  - FileContentType.active.each do |fc|
    .btn-group
      = link_to create_template_uploads_path(:file_content_type_id => fc.id), :title => 'Export assets to template', :role => 'button', :class => "btn btn-default selected-action" do
        %i.fa.fa-download.fa-1-5x
        = fc.name
